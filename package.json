{
  "name": "angorhub-sdk",
  "version": "1.0.0",
  "description": "SDK for interacting with AngorHub API",
  "main": "dist/cjs/angorhub-sdk.cjs.js",
  "module": "dist/esm/angorhub-sdk.esm.js",
  "browser": "dist/browser/angorhub-sdk.bundle.js",
  "types": "dist/esm/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:browser && npm run build:bundle",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:browser": "tsc -p tsconfig.browser.json",
    "build:bundle": "rollup -c",
    "build:watch": "rollup -c -w",
    "demo": "npx http-server -o /demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angorhub/angorhub-sdk.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/angorhub/angorhub-sdk/issues"
  },
  "homepage": "https://github.com/angorhub/angorhub-sdk#readme",
  "dependencies": {
    "@nostr-dev-kit/ndk": "^2.14.33",
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "http-server": "^14.1.1",
    "jest": "^30.0.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
